<form action="/cohorts" method="POST">
    <div class="form-group">
      <label for="logo_url">Logo Url</label>
      <input type="url" name= "logo_url" class="form-control" placeholder="Enter Logo url">
    </div>
    <div class="form-group">
      <label for="name">Name</label>
      <input type="text" name="name" class="form-control" placeholder="Name">
    </div>
    <div class="form-group">
      <label for="members">Members</label>
      <textarea cols="50" rows="4" class="form-control" name="members" placeholder="Mike Bob Bill Sally Sara Maude"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
